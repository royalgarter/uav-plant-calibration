{
  "name": "@sigodenjs/fisheye",
  "version": "0.1.3",
  "description": "A opencv fisheye camera model bindings for Node.js",
  "main": "index.js",
  "scripts": {
    "build:dev": "node-gyp -j 8 rebuild --debug",
    "build": "node-gyp -j 8 rebuild",
    "clean": "node-gyp clean",
    "example": "node example/index.js example/samples/IMG-0.jpg out.jpg",
    "install:opencv": "sudo apt install libopencv-dev python3-opencv"
  },
  "keywords": [
    "opencv",
    "fisheye",
    "camera",
    "calibrate",
    "undistort"
  ],
  "author": "Sigoden Huang <sigoden@gmail.com> (https://sigoden.com/)",
  "license": "MIT",
  "gypfile": true,
  "files": [
    "src",
    "utils",
    "binding.gyp",
    "index.d.ts",
    "index.js"
  ],
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "node-addon-api": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.9.3",
    "node-gyp": "^10.0.0"
  }
}
